{
  "name": "zillow-webpack",
  "version": "1.0.0",
  "description": "Zillow Webpack Challange",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dev": "NODE_ENV=dev npx webpack",
    "analyze:dev": "npx webpack-dev-server --config config/webpack.analyze.js",
    "server:dev": "npx webpack-dev-server --config config/webpack.dev.js --progress --profile --watch ",
    "build:prod": "npx webpack --config ./config/webpack.prod.js",
    "server:prod": "npx http-server dist --cors --proxy http://localhost:8080/index.html"
  },
  "repository": {
